{
	"name": "Yale Grade Chrome Extension",
	"description": "Chrome Extension that looks up the Yale Grade of the company owning the web page",
	"version": "0.0.1",
	"manifest_version": 3,
	"icons": {
		"16": "logo.png",
		"48": "logo.png",
		"128": "logo.png"
	},
	"action": {
		"default_title": "Yale Grade Chrome Extension",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": ["https://www.google.com/"],
			"js": ["contentScript.js"]
		}
	],
	"permissions": ["storage"]
}
